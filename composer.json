{
	"name": "exsyst/rest-bundle",
	"type": "library",
	"description": "Provides several helpers to build your rest API",
	"license": "Apache-2.0",
	"authors": [{
		"name": "EXSyst"
	}],
	"require": {
		"php": ">=5.5",
		"symfony/framework-bundle": "^2.7|^3.0",
		"symfony/serializer": "^2.7|^3.0",
		"exsyst/rest": "^0.0.6@dev"
	},
	"require-dev": {
		"symfony/phpunit-bridge": "~2.7",
		"symfony/browser-kit": "^2.7",
		"symfony/validator": "~2.7",
		"symfony/serializer": "^2.7",
		"symfony/property-access": "^2.7",
		"jms/serializer-bundle": "^1.0",
		"phpoption/phpoption": "^1.1"
	},
	"suggest": {
		"symfony/serializer": "For using the rest controller shortcuts.",
		"jms/serializer-bundle": "For using the rest controller shortcuts."
	},
	"autoload": {
		"psr-4": {
			"EXSyst\\Bundle\\RestBundle\\": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.x-dev"
		}
	}
}
